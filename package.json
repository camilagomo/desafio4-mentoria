{
  "name": "auth-api-web-cypress",
  "version": "1.0.0",
  "description": "API de Autenticação com Interface Web e Testes Cypress",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:api": "mocha tests/api/**/*.test.js --timeout 10000",
    "test:api:watch": "mocha tests/api/**/*.test.js --watch --timeout 10000",
    "test:api:ci": "mocha tests/api/**/*.test.js --timeout 10000 --reporter mochawesome --reporter-options reportDir=mochawesome-report,reportFilename=api-test-report",
    "test:report": "mochawesome-merge mochawesome-report/*.json > mochawesome-report/combined-report.json && mochawesome-report-generator mochawesome-report/combined-report.json -o mochawesome-report",
    "cypress:open": "npx cypress open",
    "cypress:run": "npx cypress run",
    "cypress:install": "npx cypress install",
    "test:e2e": "npx cypress run",
    "test:e2e:dev": "concurrently \"npm run dev\" \"npx wait-on http://localhost:3001 && npx cypress open\"",
    "test:e2e:ci": "concurrently \"npm start\" \"npx wait-on http://localhost:3001 && npx cypress run\" --kill-others --success first",
    "test:all": "npm run test:api && npm test && npm run test:e2e",
    "dev:alt": "PORT=3002 nodemon src/server.js",
    "start:alt": "PORT=3002 node src/server.js"
  },
  "keywords": [
    "nodejs",
    "express",
    "authentication",
    "api",
    "cypress",
    "testing",
    "web"
  ],
  "author": "Camila Monteiro e Leda Pires",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0"
  },
  "devDependencies": {
    "chai": "^4.3.7",
    "chai-http": "^4.3.0",
    "concurrently": "^8.2.2",
    "cypress": "^14.5.3",
    "jest": "^29.7.0",
    "mocha": "^10.8.2",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^4.2.1",
    "mochawesome-report-generator": "^6.2.0",
    "nodemon": "^3.0.2",
    "supertest": "^6.3.3",
    "wait-on": "^7.2.0"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
