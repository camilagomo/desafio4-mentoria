{
  "name": "auth-api-web-cypress",
  "version": "1.0.0",
  "description": "API de Autenticação com Interface Web e Testes Cypress",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:api": "mocha --timeout 10000 --exit --reporter mochawesome --reporter-options reportDir=mochawesome-report,reportFilename=api-test-report",
    "test:api:watch": "mocha --watch --timeout 10000 --reporter mochawesome --reporter-options reportDir=mochawesome-report,reportFilename=api-test-report",
    "test:api:ci": "mocha --config .mocharc.ci.js",
    "test:report": "mochawesome-merge mochawesome-report/*.json > mochawesome-report/combined-report.json && mochawesome-report-generator mochawesome-report/combined-report.json -o mochawesome-report",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "test:e2e": "cypress run",
    "test:all": "npm run test:api && npm test && npm run test:e2e"
  },
  "keywords": [
    "nodejs",
    "express",
    "authentication",
    "api",
    "cypress",
    "testing",
    "web"
  ],
  "author": "Camila Monteiro e Leda Pires",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "swagger-ui-express": "^5.0.0",
    "swagger-jsdoc": "^6.2.8"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "jest": "^29.7.0",
    "cypress": "^13.6.0",
    "mocha": "^10.2.0",
    "chai": "^4.3.7",
    "supertest": "^6.3.3",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^4.2.1",
    "mochawesome-report-generator": "^6.2.0"
  },
  "engines": {
    "node": ">=14.0.0"
  }
} 